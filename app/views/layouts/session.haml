- content_for :app_head do
  = stylesheet_link_tag "layouts/session-layout", :media => "all"
  = javascript_include_tag "layouts/session-layout"
  = yield :head
- content_for :app_content do
  #top-bar
    = link_to( image_tag("logo.jpg", width: "50px", class: "logo-image"), "/")
  = yield :content
= render :template => 'layouts/application'
